{
  "name": "cloudypress/framework",
  "description": "Framework for building WordPress plugins with developer-friendly API routes and database ORM support",
  "type": "library",
  "version": "0.1.0",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "wordpress",
    "plugin",
    "framework",
    "orm",
    "database",
    "query-builder",
    "routing",
    "api",
    "rest",
    "developer-friendly",
    "cloudypress"
  ],
  "autoload": {
    "psr-4": {
      "CloudyPress\\Core\\": "src/",
      "CloudyPress\\Database\\": "database/",
      "CloudyPress\\Woocommerce\\": "wooocommerce/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require": {
    "php": ">=8.2",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.3"
  }
}
